{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@db": [
        "src/db/db.ts"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@model": [
        "src/models/index.ts"
      ]
    },
    "jsx": "preserve"
  }
}